{
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 60
    }
  },
  "env": {
    "OPENROUTER_API_KEY": "@openrouter-key",
    "CHUTES_API_KEY": "@chutes-key",
    "CHUTES_API_BASE_URL": "@chutes-api-url",
    "CHUTES_IMAGE_API_URL": "@chutes-image-url",
    "CHUTES_VIDEO_API_URL": "@chutes-video-url",
    "OPENROUTER_APP_NAME": "@openrouter-app-name",
    "NEXT_PUBLIC_APP_URL": "@app-url"
  },
  "regions": ["iad1"],
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install"
}